.color-one {
  @import 'color_package_one';

$button: #04bfbf;
$buttonHover: #cafcd8;

// font's below
$josefinSlab: 'Josefin Slab', serif;
$specialElite: 'Special Elite', cursive;
$jacquesFrancoisShadow: 'Jacques Francois Shadow', cursive;

// DELETE THIS AFTER COLOR HAS BEEN SELECTED, UNCOMMENT THIS FROM STYLE.SCSS
.panel {
  border-color: $mainColor !important;
  box-shadow:5px 5px 2px rgba(122,116,116,0.15) !important;
  font-size: 1.2em;
}

.panel-body {
  background-color: #fff;
}

.panel-heading {
  border-bottom: $mainColor !important;
  background-color: $mainColor !important;
  height: 60px;
  font-weight: 900;
  color: $contrastText !important;
}

.panel-title {
  color: $titleText !important;
  font-size: 2.5em !important;

}

.panel_buttons {
  text-align: center;
  padding: 5px;
}

// - - - - - - - - - - DELETE THIS AFTER COLOR HAS BEEN SELECTED, UNCOMMENT THIS FROM STYLE.SCSS ABOVE - - - - -

// styling for buttons
.buttonStyle {
  -webkit-box-shadow: 2px 2px 5px 0px rgba(0,0,0,0.75);
  -moz-box-shadow: 2px 2px 5px 0px rgba(0,0,0,0.75);
  box-shadow: 2px 2px 5px 0px rgba(0,0,0,0.75); 
}

.landing_page_header {
  background-color: $mainColor;
  width: 100%;
  height: 400px;
  border-bottom: 10px solid $contrastColor;
  text-align: center; 
}

.landing_page_body {
  padding-top: 2.5%;
}

.landing_page_header h1 {
  font-weight: 100;
  font-size: 400%;
  font-family: $josefinSlab;
}

.landing_page_header_text {
  color: $darkBackgroundText;
  font-size: 175%;
  font-family: $josefinSlab;
}

.landing_page_text_unique {
  color: $contrastText;
  font-weight: 900;
}

#box {
  color: #fff;
  width: 125px;
  height: 125px;
  border: 1px double #fff;
  outline: 2px solid #fff;
  outline-offset: -9px;
}

.fancy {
    line-height: 0.5;
    text-align: center;
    margin-bottom: 40px;
    margin-top: 20px;
}

.fancy span {
  display: inline-block;
  position: relative;
}

.fancy span:before,
.fancy span:after {
    content: "";
    position: absolute;
    height: 5px;
    border-bottom: 1px solid #f1f1f1;
    top: 50px;
    width: 400%;
}
.fancy span:before {
    right: 100%;
    margin-right: 10px;
}
.fancy span:after {
    left: 100%;
    margin-left: 10px;
}

// body
.landing_page_titles {
  padding-bottom: 10px;
}

.landing_page_section_one {
  padding-top: 40px;
  padding-bottom: 40px;
  text-align: center;
  font-size: 175%;
  font-family: $josefinSlab;
  background-color: $mainBackGroundColor;
}

.flag {
  display: inline-block;
  color: $contrastText;
  font-size: 75% !important;
  font-weight: 900;
  -ms-transform: rotate(-30deg); /* IE 9 */
  -webkit-transform: rotate(-30deg); /* Safari */
  transform: rotate(-30deg);
}

.push {
  height: 250px; /* .push must be the same height as .footer */
}

.landing_page_footer {
  background-color: $mainColor;
  width: 100%;
  height: 150px;
  border-top: 10px solid $contrastColor;
  text-align: center; 
}

.footer-columns {

}

.landing_page_footer_title {
  color: $boldColor;
  margin-top: 10px;
  font-size: 125%;
  font-family: $josefinSlab;
}

.landing_page_contact{

}

.landing_page_date {
  color: $generalText;
  font-size: 80%;
}

.btn-success {
  background-color: $contrastColor;
}

.landing_page_arrow_box {
  position: relative
  
}

.landing_page_arrow {
  margin-left: -10px;
}

.landing_page_arrow_text {
  position: absolute;
  color: #000000;
  margin-top: -24px;
  margin-left: 40px;
  font-size: 300%;
  font-family: $specialElite;
}

// CUSTOM BUTTON BELOW for yellow

.btn-login { 
  color: #000; 
  background-color: $button; 
  border-color: $button;
} 
 
.btn-login:hover, 
.btn-login:focus, 
.btn-login:active, 
.btn-login.active, 
.open .dropdown-toggle.btn-login { 
  color: #000; 
  background-color: $buttonHover; 
  border-color: #000000; 
} 
 
.btn-login:active, 
.btn-login.active, 
.open .dropdown-toggle.btn-login { 
  background-image: none; 
} 
 
.btn-login.disabled, 
.btn-login[disabled], 
fieldset[disabled] .btn-login, 
.btn-login.disabled:hover, 
.btn-login[disabled]:hover, 
fieldset[disabled] .btn-login:hover, 
.btn-login.disabled:focus, 
.btn-login[disabled]:focus, 
fieldset[disabled] .btn-login:focus, 
.btn-login.disabled:active, 
.btn-login[disabled]:active, 
fieldset[disabled] .btn-login:active, 
.btn-login.disabled.active, 
.btn-login[disabled].active, 
fieldset[disabled] .btn-login.active { 
  background-color: #FFCC33; 
  border-color: #FFCC33; 
} 
 
.btn-login .badge { 
  color: #FFCC33; 
  background-color: #000; 
}

.not-a-memeber {
  padding: 10px;
  font-weight: 900;
  font-size: 110%;
  font-color: $contrastText;
}
}
